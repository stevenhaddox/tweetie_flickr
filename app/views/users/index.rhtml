<div id="users">
  <%- if !@users.blank? -%>
    <%- for user in @users -%>
      <% div_for user do %>
        <p><%= link_to user.twitter_username, user_path(user) %></p>
      <% end -%>
    <%- end -%>
  <%- else -%>
    <p>There are no registered users at this time.</p>
  <%- end -%>
</div>
<%= will_paginate @users %>